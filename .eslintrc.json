{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "plugins": ["prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": ["error", {"argsIgnorePatern": "next"}],
    "no-param-reassign": "off",
    "camelcase": "off",
    "class-methods-use-this": "off",
    "global-require": "off",
    "no-console": "off"
  }
}
